    dc:identifier="http://www.roadtrip20.com/2007_09_25-social-media-marketing-efforts-for-roadtrip20com-part-01.html"
    dc:title="Social Media Marketing Efforts for RoadTrip20.com, Part 01"
    trackback:ping="http://www.roadtrip20.com/2007_09_25-social-media-marketing-efforts-for-roadtrip20com-part-01.html/trackback" />
